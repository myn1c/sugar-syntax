class People{
    def People(name: String, age: Int, pet?: People.Pet){
        this.name = name
        this.age = age
        if pet != None {
            this.pet = pet
        }
    }

    class Pet{
        def Pet(name: String, age: Int){
            this.name = name
            this.age = age
        }
    }
}

class Child{
    def Child(name: String, age: Int, weight: String) : People(name, age){
        this.weight = weight
    }
}

def main(args: String[]){
    people = People("Yaroslav", 19, People.Pet("Tom", 7))
}
